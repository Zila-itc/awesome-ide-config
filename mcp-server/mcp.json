{
  "mcpServers": {
    "mcp-mui": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@mui/mcp@0.1.0"
      ],
      "cwd": "${workspaceFolder}",
      "env": {},
      "dev": {}
    },
    "Ant Design Components": {
      "command": "npx",
      "args": [
        "@jzone-mcp/antd-components-mcp"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${workspaceFolder}"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    }
  }
}
